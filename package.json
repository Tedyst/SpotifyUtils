  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@devexpress/dx-react-chart": "^2.7.1",
    "@devexpress/dx-react-chart-material-ui": "^2.7.1",
    "@devexpress/dx-react-core": "^2.7.1",
    "@material-ui/core": "^4.11.0",
