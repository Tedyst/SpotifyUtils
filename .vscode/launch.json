{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Launch golang",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}",
            "args": [
                "-Debug=true"
            ]
        }
    ]
}