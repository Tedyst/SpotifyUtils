    "build-sw": "node ./src/sw-build.js",
    "docker-package": "docker buildx build --platform linux/amd64,linux/arm64,linux/arm/v7 -t tedyst/spotifyutils --push -f Dockerfile .",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:5000/",
  "devDependencies": {
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "react-scripts": "^3.4.3",
    "workbox-build": "^4.3.1"
  }
